#include <iostream>
#include <iomanip>

int main() {
    double points;

    std::cout << "Введіть кількість балів (0.0 ... 9.0): ";
    std::cin >> points;

    
    if (points < 0.0 || points > 9.0) {
        std::cout << "Неправильна кількість пунктів!" << std::endl;
    } else {
        if (points <= 4.4)
            std::cout << "Оцінка: незадовільна (2.0)" << std::endl;
        else if (points <= 5.2)
            std::cout << "Оцінка: задовільна (3.0)" << std::endl;
        else if (points <= 6.2)
            std::cout << "Оцінка: вище задовільної (3.5)" << std::endl;
        else if (points <= 7.2)
            std::cout << "Оцінка: хороша (4.0)" << std::endl;
        else if (points <= 8.2)
            std::cout << "Оцінка: вище добре (4.5)" << std::endl;
        else
            std::cout << "Оцінка: дуже добре (5.0)" << std::endl;
    }

    return 0;
